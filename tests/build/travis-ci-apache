<VirtualHost *>
	ServerAdmin tests@inboundnow.com
	DocumentRoot "/var/www/inboundsoon.dev"
	ServerName inboundsoon.dev
	ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined
	<Directory "/var/www/inboundsoon.dev">
	  Order allow,deny
	  Allow from all
	</Directory>
</VirtualHost>
  